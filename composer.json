{
    "name": "tolkam/asset",
    "description": "Assets resolution and versioning inspired by Symfony Asset",
    "type": "library",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Tolkam\\Asset\\": "src"
        }
    },
    "suggest": {
        "tolkam/asset-twig": "Twig integration"
    },
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "ext-mbstring": "*"
    }
}
